import java.util.Scanner;
class HelloWorld {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        System.out.println("Enter A Number :");
        int num=scanner.nextInt();
        int total=1;
         for(int x=1;x<=num;x++){
            total=total*x;
        }
        System.out.println("the factorial is:"+ total);
    }
    
}
